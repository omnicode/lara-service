{
	"name": "omnicode/lara-service",
	"description": "Service layer for CRUD",
	"keywords": ["laravel", "crud", "service layer"],
	"license": "MIT",
	"authors": [
		{
			"name": "Omnicode"
		}
	],
	"require": {
		"laravel/framework": "5.3.* || 5.4.* || 5.5.*",
		"omnicode/lara-repo": "~0.1 || "~0.0"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
		"phpunit/phpunit": "~4.0|~5.0",
		"php-mock/php-mock": "1.0.1"
	},
	"autoload": {
		"psr-4": {
			"LaraService\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
